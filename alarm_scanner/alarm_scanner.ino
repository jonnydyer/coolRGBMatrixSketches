void update(const uint16_t *img) {
  for(int i=0; i<32; i++){
    for(int j=0; j<16; j++){
      matrix.drawPixel(i, j, img[j * 32 + i]);
    }
  }
}

void loop() {
  uint32_t t;
  while(((t = millis()) - prevTime) < (1000 / FPS));
  prevTime = t;

  update(abby);

  delay(1000);

  update(lucy);

  delay(1000);

  update(finley);

  delay(1000);

  matrix.swapBuffers(false);
}
